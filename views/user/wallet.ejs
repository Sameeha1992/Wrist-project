
            <!-- Wallet History -->
            <div class="tab-pane fade" id="wallet-history" role="tabpanel" aria-labelledby="orders-tab">
                <div class="card">
                  <div class="card-header">
                    <h5 class="mb-0">Wallet History</h5>
                  </div>
                  <div class="card-body">
                    <div class="table-responsive">
                      <table class="table">
                        <thead>
                          <tr>
                            <th>Date</th>
                            <th>Status</th>
                            <th>Amount</th>
                          </tr>
                        </thead>
                        <tbody>
                          <% const dates = ['2023-05-01', '2023-05-15', '2023-06-01'];
                             const statuses = ['Credit', 'Debit', 'Credit'];
                             const amounts = [100, 50, 200];
                             for(let i=0; i<3; i++) { %>
                            <tr>
                              <td><%= dates[i] %></td>
                              <td><%= statuses[i] %></td>
                              <td>$<%= amounts[i] %>.00</td>
                            </tr>
                          <% } %>
                        </tbody>
                      </table>
                    </div>
                  </div>
                </div>
              </div>
  